<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Terceira pergunta</title>
  <style>
  img { 
  	width: 500px;
	height:auto;
    float: left;
	margin-right: 10px;
	margin-bottom: 10px;
  }
  </style>
  <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
</head>
<body>
 
<div id="images"></div>
 
<script>
(function() {
  var flickerAPI = "http://api.flickr.com/services/feeds/photos_public.gne?jsoncallback=?";
  $.getJSON( flickerAPI, {
    tags: "foto",
    format: "json"
  })
    .done(function( data ) {
      $.each( data.items, function( i, item ) {
        $( "<img>" ).attr( "src", item.media.m ).appendTo( "#images" );
        if ( i === 5 ) {
          return false;
        }
      });
    });
})();
</script>
 
</body>
</html>
